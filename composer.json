{
    "name": "irmmr/rtlcss",
    "type": "library",
    "description": "PHP Module for Converting LTR CSS to RTL",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "keywords": [
        "rtlcss",
        "RTLCss",
        "php-rtlcss"
    ],
    "authors": [
        {
            "name": "marmar8x",
            "email": "marmar8x.code@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "sabberworm/php-css-parser": "^8.7"
    },
    "autoload": {
        "psr-4": {
            "Irmmr\\RTLCss\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "11",
        "ext-json": "*"
    }
}
